    <Member MemberName="MaximumTransferTime">
      <MemberSignature Language="C#" Value="public TimeSpan MaximumTransferTime { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.TimeSpan MaximumTransferTime" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Rtc.Collaboration.CallTransferOptions.MaximumTransferTime" />
